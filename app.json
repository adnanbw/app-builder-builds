{
  "expo": {
    "name": "App Builder Builds",
    "slug": "app-builder-builds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appbuilder.template"
    },
    "android": {
      "package": "com.appbuilder.template",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "07374dba-2296-451e-b1c3-10fc1dc24aba"
      }
    },
    "owner": "adnanbw",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
             "gradleVersion": "8.0.2"
          }
        }
      ]
    ]
  }
}
