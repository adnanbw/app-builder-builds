      "bundleIdentifier": "com.appbuilder.template"
    },
    "android": {
      "package": "com.appbuilder.template",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "07374dba-2296-451e-b1c3-10fc1dc24aba"
      }
    },
    "owner": "adnanbw"
  }
